{
  "version": 2,
  "buildCommand": "",
  "installCommand": "",
  "outputDirectory": "public",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "arenastreams.com"
        }
      ],
      "destination": "https://arenastreams.site/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.arenastreams.com"
        }
      ],
      "destination": "https://arenastreams.site/:path*",
      "permanent": true
    }
  ]
}

